  <tr>
%   }
<td>
 <%= $img->{name} %> &nbsp;&nbsp;&nbsp; <%= $img->{size} %>: <br>
 <img width=200 height=200 src="<%= $img->{src} %>" /><br> 
</td>
%   if (($i % 5) == 0) {
  </tr>
%   }
% }
</table>

